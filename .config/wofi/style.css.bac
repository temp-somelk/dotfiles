#window {background-color: #222222; color: #ffffff; border-style: none; font-family: SourceSansPro; font-size: 14px;}
#outer-box {background-color: #222222; color: #ffffff;}

/* unable to disable borders for searchbox and entry, so adding boder settings */
#input {background-color: #222222; color: #ffffff; border-radius: 0px; border-width: 2px; border-color: #222222;}
#entry:selected {background-color: #9cffa2; border-radius: 0px;}
#text:selected {color: #000000;}
