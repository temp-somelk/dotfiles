[options]
allow_bold = true
audible_bell = true
bold_is_bright = true
cell_height_scale = 1.0
cell_width_scale = 1.0
clickable_url = true
dynamic_title = true
font = JetBrainsMono 10
fullscreen = true
icon_name = terminal
mouse_autohide = true
scroll_on_output = false
scroll_on_keystroke = true
# Length of the scrollback buffer, 0 disabled the scrollback buffer
# and setting it to a negative value means "infinite scrollback"
scrollback_lines = 10000
search_wrap = true
urgent_on_bell = true
hyperlinks = false

# $BROWSER is used by default if set, with xdg-open as a fallback
browser = firefox

# "system", "on" or "off"
cursor_blink = system

# "block", "underline" or "ibeam"
cursor_shape = block

# Hide links that are no longer valid in url select overlay mode
filter_unmatched_urls = true

# Emit escape sequences for extra modified keys
modify_other_keys = true

# set size hints for the window
#size_hints = false

# "off", "left" or "right"
scrollbar = off

[colors]
cursor = #e5e9f0
cursor_foreground = #0f111a

background = #0f111a
foreground = #e5e9f0
foreground_bold = #e5e9f0

# highlight = #222222

color0 = #1C1F31
color1 = #BF616A
color2 = #A3BE8C
# color2 = #3A575C
color3 = #EBCB8B
color4 = #81A1C1
color5 = #B48EAD
color6 = #88C0D0
color7 = #E5E9F0
color8 = #4C566A
color9 = #BF616A
color10 = #A3BE8C
# color10 = #3A575C
color11 = #EBCB8B
color12 = #81A1C1
color13 = #B48EAD
color14 = #88C0D0
color15 = #E5E9F0

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
#border_width = 0.5
#roundness = 2.0

